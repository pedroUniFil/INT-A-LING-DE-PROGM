import java.util.Scanner;

public class ex2 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String nome;
        String senha;
        System.out.println("Insira o nome de usario: ");
        nome = sc.nextLine();

        System.out.println("Insira a senha");
        senha = sc.nextLine();

        while(senha.equals(nome)){
            System.out.println("Nome e senha iguais, insira outra senha");
            senha = sc.nextLine();
        }
    }
}
