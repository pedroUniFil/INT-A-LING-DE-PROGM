import java.util.Scanner;

public class ex1 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int a;

        do {
            System.out.println("Informe um numero entre 0 e 10");
            a = sc.nextInt();


        }while((a < 0) || (a >10));


        }
    }
